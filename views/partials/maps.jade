div(class="square-container")
    div(class="square",ng-repeat="l in matches",ng-class="{'square-container-clicked': l.clicked}")
        a(ng-click="showUsers(l)") {{l.Name}}
div(class="big square")
    div(class="square",ng-repeat="u in matchedUsers")
        a(ng-mouseover="showMenu()") {{u.firstName}}
    div(class="square",ng-show="matchedUsers && matchedUsers.length <=0")
        span There are no matches around your area. You can still connect to people based on the geolocations